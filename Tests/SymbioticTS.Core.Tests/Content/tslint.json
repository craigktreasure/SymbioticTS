{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "member-ordering": {
            "options": {
                "order": "fields-first"
            }
        },
        "object-literal-shorthand": false,
        "trailing-comma": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}